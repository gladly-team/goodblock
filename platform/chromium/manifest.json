{
    "manifest_version": 2,

    "name": "Goodblock",
    "version": "0.0.19",

    "description": "Goodblock is no longer actively maintained. Please find another extension, such as uBlock Origin.",
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },

    "author": "The Goodblock Development Team",
    "content_scripts": [],
    "incognito": "split",
    "minimum_chrome_version": "22.0",
    "permissions": [
        "contextMenus",
        "privacy",
        "storage",
        "tabs",
        "alarms",
        "unlimitedStorage",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "cookies",
        "history",
        "topSites",
        "browsingData",
        "notifications"
    ],
    "short_name": "Goodblock"
}
